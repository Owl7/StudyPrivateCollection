#define UIColorFromRGB(rgbValue) [UIColor colorWithRed:((float)((rgbValue & 0xFF0000) >> 16))/225.0 green:((float)((rgbValue & 0x00FF00) >> 8))/225.0 blue:((float)(rgbValue & 0x0000FF))/225.0 alpha:1.0]

#define UIColorFromRGBA(rgbValue, alphaValue) [UIColor colorWithRed:((float)((rgbValue & 0xFF0000) >> 16))/225.0 green:((float)((rgbValue & 0x00FF00) >> 8))/225.0 blue:((float)(rgbValue & 0x0000FF))/225.0 alpha:alphaValue]

